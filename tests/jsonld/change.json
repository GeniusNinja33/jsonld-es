{
   "group": "change",
   "tests": [{
      "name": "change context",
      "type": "change",
      "input": {
         "@context": {
            "@coerce": {
               "xsd:anyURI": ["e:authored", "e:contains"]
            },
            "d": "http://purl.org/dc/elements/1.1/",
            "e": "http://example.org/vocab#",
            "f": "http://xmlns.com/foaf/0.1/"
         },
         "@": [{
            "@": "http://example.org/test#chapter",
            "d:description": "Fun",
            "d:title": "Chapter One"
         }, {
            "@": "http://example.org/test#jane",
            "e:authored": "http://example.org/test#chapter",
            "f:name": "Jane"
         }, {
            "@": "http://example.org/test#john",
            "f:name": "John"
         }, {
            "@": "http://example.org/test#library",
            "e:contains": {
               "@": "http://example.org/test#book",
               "d:contributor": "Writer",
               "d:title": "My Book",
               "e:contains": "http://example.org/test#chapter"
            }
         }]
      },
      "context": {
         "@coerce": {
            "xsd:anyURI": ["ex:authored", "ex:contains"]
         },
         "dc": "http://purl.org/dc/elements/1.1/",
         "ex": "http://example.org/vocab#",
         "foaf": "http://xmlns.com/foaf/0.1/",
         "xsd": "http://www.w3.org/2001/XMLSchema#"
      },
      "expect": {
         "@context": {
            "@coerce": {
               "xsd:anyURI": ["ex:authored", "ex:contains"]
            },
            "dc": "http://purl.org/dc/elements/1.1/",
            "ex": "http://example.org/vocab#",
            "foaf": "http://xmlns.com/foaf/0.1/",
            "xsd": "http://www.w3.org/2001/XMLSchema#"
         },
         "@": [{
            "@": "http://example.org/test#chapter",
            "dc:description": "Fun",
            "dc:title": "Chapter One"
         }, {
            "@": "http://example.org/test#jane",
            "ex:authored": "http://example.org/test#chapter",
            "foaf:name": "Jane"
         }, {
            "@": "http://example.org/test#john",
            "foaf:name": "John"
         }, {
            "@": "http://example.org/test#library",
            "ex:contains": {
               "@": "http://example.org/test#book",
               "dc:contributor": "Writer",
               "dc:title": "My Book",
               "ex:contains": "http://example.org/test#chapter"
            }
         }]
      }
   }, {
      "name": "native types",
      "type": "change",
      "input": {
         "@": "http://example.org/test",
         "@context": {
            "d": "http://purl.org/dc/elements/1.1/",
            "e": "http://example.org/vocab#",
            "f": "http://xmlns.com/foaf/0.1/"
         },
         "e:bool": true,
         "e:double": 1.230000e+00,
         "e:double-zero": 0.000000e+00,
         "e:int": 123
      },
      "context": {
         "ex": "http://example.org/vocab#"
      },
      "expect": {
         "@context": {
            "ex": "http://example.org/vocab#"
         },
         "@": "http://example.org/test",
         "ex:bool": true,
         "ex:double": 1.230000e+00,
         "ex:double-zero": 0.000000e+00,
         "ex:int": 123
      }
   }, {
      "name": "native types w/ coerce map",
      "type": "change",
      "input": {
         "@": "http://example.org/test",
         "@context": {
            "e": "http://example.org/vocab#"
         },
         "e:bool": true,
         "e:double": 1.230000e+00,
         "e:int": 123
      },
      "context": {
         "@coerce": {
            "xsd:boolean": "ex:bool",
            "xsd:double": "ex:double",
            "xsd:integer": "ex:int"
         },
         "ex": "http://example.org/vocab#",
         "xsd": "http://www.w3.org/2001/XMLSchema#"
      },
      "expect": {
         "@context": {
            "@coerce": {
               "xsd:boolean": "ex:bool",
               "xsd:double": "ex:double",
               "xsd:integer": "ex:int"
            },
            "ex": "http://example.org/vocab#",
            "xsd": "http://www.w3.org/2001/XMLSchema#"
         },
         "@": "http://example.org/test",
         "ex:bool": true,
         "ex:double": 1.230000e+00,
         "ex:int": 123
      }
   }, {
      "name": "with coerce",
      "type": "change",
      "input": {
         "@": "http://example.org/test",
         "http://example.org/test#int": {
            "@datatype": "http://www.w3.org/2001/XMLSchema#integer",
            "@literal": "123"
         }
      },
      "context": {
         "@coerce": {
            "xsd:integer": "ex:int"
         },
         "ex": "http://example.org/test#",
         "xsd": "http://www.w3.org/2001/XMLSchema#"
      },
      "expect": {
         "@context": {
            "@coerce": {
               "xsd:integer": "ex:int"
            },
            "ex": "http://example.org/test#",
            "xsd": "http://www.w3.org/2001/XMLSchema#"
         },
         "@": "http://example.org/test",
         "ex:int": 123
      }
   }]
}